{
  "name": "Out of Stock Alerts",
  "version": "1.0.0",
  "description": "Domo procode app for monitoring and acknowledging out-of-stock product alerts with Tabulator table",
  "main": "index.html",
  "width": 3,
  "height": 2,
  "fileName": "manifest.json",
  "mapping": [
    {
      "alias": "product-alerts",
      "dataSetId": "YOUR_DATASET_ID_HERE",
      "fields": []
    }
  ],
  "syncEnabled": true,
  "syncConfig": {
    "collections": [
      {
        "collectionName": "acknowledged-alerts",
        "targetDatasetId": "YOUR_ACKNOWLEDGMENTS_DATASET_ID_HERE",
        "syncFrequency": "realtime",
        "columnMapping": {
          "productId": "product_id",
          "productName": "product_name",
          "acknowledgedAt": "acknowledged_at",
          "acknowledgedBy": "acknowledged_by"
        }
      }
    ]
  },
  "id": "5e067702-328a-4d47-9fbe-dca1c34eca5e"
}
